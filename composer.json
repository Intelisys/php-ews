{
  "name": "intelisys/php-ews",
  "description": "Library for communicating with Exchange Web Services. Based on James Armes php ews package.",
  "homepage": "https://github.com/Intelisys/php-ews",
  "license": "MIT",
  "authors": [
    {
      "name": "Will Scharff",
      "email": "wscharff@intelisys.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.3.9",
    "ext-soap": "*",
    "jamesiarmes/php-ntlm": "~1.0.0"
  },
  "require-dev": {
    "mustache/mustache": "~2.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Intelisys\\PhpEws\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
